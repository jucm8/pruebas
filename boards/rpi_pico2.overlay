/ {
    aliases {
        ethernet0 = &w5500;
    };
};

&spi0 {
    status = "okay";

    w5500: ethernet@0 {
        compatible = "wiznet,w5500";
        reg = <0>;
        spi-max-frequency = <20000000>;
        int-gpios = <&gpio0 21 GPIO_ACTIVE_LOW>;
        reset-gpios = <&gpio0 20 GPIO_ACTIVE_LOW>;
        status = "okay";
    };
};

